.index-container {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-bottom: 150rpx;
  background: white;
}
.title{
  background: rgb(92,146,239);
  height: 220rpx;
  line-height: 220rpx;
  text-align: center;
  color: white;
  font-size:40rpx;
}
.receipt-code__list{
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.receipt-code__list .receipt-code__item{
  width: 50%;
   align-items: center;
  }
.receipt-code__list .receipt-code__item view{
   height: 300rpx;
   box-sizing: border-box;
   box-shadow: 0 0 20px rgba(0,0,0,.1); 
   margin: 15rpx 0;
}
.receipt-code__list .icon{
  width: 40rpx;
  height:40rpx;
  display: block;
  padding-top: 100rpx;
  margin-bottom: 10rpx;
}
.receipt-code__item view text{
  display: block;
  width: 230rpx;
  text-align: center;
  font-size: 32rpx;
}
.receipt-code__item .icon,.receipt-code__item view text{
  position: relative;
  left:50%;
  transform:translateX(-50%);
}

.get-path{
  display: block;
  text-align: center;
}

.pay-name{
  width: 100%;
  text-align: center;
  margin-top: 80rpx;
}

.pay-name text,.pay-name input{
 display: inline-block;
 vertical-align: middle;
}
.pay-name input{
  margin-left: 10rpx;
  margin-right: 10rpx;
  text-align: left;
}

.btn-list{
  display: block;
  width: 100%;
  text-align: center;
  margin-top: 30rpx;
}
.btn-list button{
  display: inline-block;
  width: 40%;
  height: 80rpx;
  line-height: 80rpx;
}
.btn-list .marge-btn{
  margin-right: 20rpx;
}
.tip-text{
  margin-top: 20rpx;
}
.tip-text,.tip-text text{
  display: block;
  width: 100%;
}

.tip-text text{
  text-align: center;
}

.pay-text{
  display: block;
  width: 100%;
}
.pay-text text{
  display: block;
  text-align: center;
   margin-top: 20rpx;
   margin-bottom: 20rpx;
  }

.code-item {
  display: block;
  width: 100%;
  max-width: 800rpx;
  margin: 0 auto;
  padding-bottom: 2rpx;
  background-color: #5c92ef;
}
.code-title {
    display: flex;
    height: 200rpx;
    background-image: url("data:image/png;base64,");
    background-color: #f2f2f2;
    background-position: center;
    background-repeat: no-repeat;
    background-size: 50%;
}
 .code-area {
    display: flex;
    justify-content: center;
}
  .code-area>image {
    margin: 160rpx 0;
    width: 60%;
    min-width: 200rpx;
}


  .code-footer {
        height: 160rpx;
        text-align: center;
        background-color: #fff;
        color: #666;
        line-height: 160rpx;
        font-size: 40rpx;
        margin: -4rpx 4rpx 4rpx 4rpx;
    }




  .tab-bar{
    position:fixed;
    left:0;
    right:0;
    bottom: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content:center;
    text-align: center;
    background: white;
  }
  .tab-bar navigator{
    flex: 1;
    height: 100rpx;
  }
  .tab-bar text{
    height: 100rpx;
    background-position: top center;
    background-size: 60rpx;
    background-repeat: no-repeat;
    display: block;
    padding-top:60rpx;

  }
  .tab-bar .index.active{
     background-image:url('../assets/index_active_icon.png');
    }
    
      .tab-bar .safe{
     background-image:url('../assets/safe_icon.png');
    }
    
      .tab-bar .center{
     background-image:url('../assets/center_icon.png');
  }
